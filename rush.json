/**
 * This is the main configuration file for Rush.
 * For full documentation, please see https://rushjs.io
 */
{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.74.0",
  "pnpmVersion": "7.3.0",
  "pnpmOptions": {
    "strictPeerDependencies": true,
    "resolutionStrategy": "fewer-dependencies",
    "preventManualShrinkwrapChanges": true,
    "useWorkspaces": true
  },
  "nodeSupportedVersionRange": ">=16.13.0 <17.0.0",
  "ensureConsistentVersions": true,
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 2,
  "repository": {
    "url": "https://github.com/parijatmishra/learn-rushjs"
  },
  "projects": [
    {
      "packageName": "libgreet",
      "projectFolder": "lib/greet",
      "shouldPublish": false
    },
    {
      "packageName": "hello-strict",
      "projectFolder": "cli/hello-strict",
      "shouldPublish": false
    }
  ]
}
